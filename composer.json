{
  "name": "ybazli/moloquent",
  "description": "A MongoDB based Eloquent model and Query builder for Laravel (Moloquent)",
  "keywords": [
    "laravel",
    "eloquent",
    "mongodb",
    "mongo",
    "database",
    "model",
    "moloquent"
  ],
  "homepage": "https://github.com/ybazli/moloquent",
  "authors": [
    {
      "name": "Moloquent",
      "homepage": ""
    },
    {
      "name": "Jens Segers",
      "homepage": "https://jenssegers.com"
    }
  ],
  "license": "MIT",
  "require": {
    "illuminate/support": "*",
    "illuminate/container": "*",
    "illuminate/database": "*",
    "illuminate/events": "*",
    "mongodb/mongodb": "^1.0.0"
  },
  "require-dev": {
    "laravel/passport": "^1.0",
    "phpunit/phpunit": "^4.0|^5.0",
    "orchestra/testbench": "^3.1",
    "mockery/mockery": "^0.9",
    "satooshi/php-coveralls": "^0.6"
  },
  "autoload": {
    "psr-4" :{
      "Moloquent\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/models",
      "tests/seeds"
    ]
  }
}
